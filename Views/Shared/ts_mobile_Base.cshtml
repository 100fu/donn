<!doctype html>
<html>

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1">
    @{
        Layout = null;
        var scriptitems = (ViewBag.Scripts as List<string>) ?? new List<string>();
        <script type="text/javascript">
            window.ViewBag =@Html.Raw(Json.Encode(ViewBag))
        </script>
        <link href="~/FES/content/css/bootstrap.min.css" rel="stylesheet" />
        <link href="~/FES/content/css/font-awesome.min.css" rel="stylesheet" />
        @*<link href = "/FES/third_parts/gridstack.min.css" rel = "stylesheet" />*@
        foreach (var item in scriptitems)
        {
            <script src="@item"></script>
        }
    }
</head>

<body>
    <script data-main="@ViewBag.ControllerPath" src="~/FES/third_parts/require.js"></script>
</body>

</html>